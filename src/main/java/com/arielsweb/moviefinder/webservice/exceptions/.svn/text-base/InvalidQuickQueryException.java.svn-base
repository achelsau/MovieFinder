package com.arielsweb.moviefinder.webservice.exceptions;

/**
 * Signals that a client arrived null or empty at the server
 * 
 * @author Ariel
 * 
 */
public class InvalidQuickQueryException extends Exception {

    private static final long serialVersionUID = -5088215105785048672L;
    
    /**
     * Contructs a new IQE
     * 
     * @param message
     *            the message to show to the user of the WS
     */
    public InvalidQuickQueryException(String message) {
	super(message);
    }
}
